            <kind>1</kind>
            <string>///Check for strokes

if ((curFrame &gt; accelFrames || curFrame &gt; numFrames) &amp;&amp; scrCheckLeftButton()) {
  //curForward = maxForward;
 // curTurn = maxTurn; 
  curFrame = 0;
  dir = 1;
  reachedDir = false;
}

